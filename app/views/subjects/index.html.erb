<p style="color: green"><%= notice %></p>

<h1>Subjects</h1>

<div id="subjects">
  <% @subjects.each do |subject| %>
    <div class = "list">
      <%= render subject %>
      <p>
        <%= link_to "Show this subject", subject %>
      </p>
    </div>
  <% end %>
</div>

<%= link_to "New subject", new_subject_path %>
